<div class='main-div'>
  <h1><%= format_date @accountability_log.dueby %><br>
  <small class='text-muted'>posted <%= format_time @accountability_log.created_at %></small></h1>
  <% if logged_in && current_user.is_admin %>
    <%= link_to 'Edit Accountability Log', edit_accountability_log_path(@accountability_log), class: 'btn btn-warning btn-sm btn-block' %>
  <% end %>
  <hr>
  <h2>Submissions</h2>
  <table class="table table-striped table-sm">
    <thead>
      <tr>
        <th>User</th>
        <th>Created At Timestamp</th>
        <th></th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <%= render partial: 'submission', collection: User.where("rank < ? AND rank > ?", 8, 0), as: :user or 'No submissions have been posted for this accountability log.'%>
    </tbody>
  </table>
</div>
